{
    "name": "rmzamora/news-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataNewsBundle, rmzamora forked",
    "keywords": ["blog", "sonata", "news", "post", "rmzamora forked"],
    "homepage": "https://sonata-project.org/bundles/news",
    "license": "MIT",
    "authors": [
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataNewsBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/symfony": "~2.4",
        "sonata-project/core-bundle": "~2.2",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/intl-bundle": "~2.1",
        "sonata-project/datagrid-bundle": "~2.2@dev",
        "jms/serializer-bundle": "~0.11",
        "nelmio/api-doc-bundle": "~2.4",
        "friendsofsymfony/rest-bundle": "~1.1",
        "rmzamora/admin-bundle": "2.4.1.*",
        "rmzamora/user-bundle":  "2.4.1.*",
        "rmzamora/media-bundle": "2.4.1.*",
        "rmzamora/formatter-bundle": "2.4.1.*",
        "rmzamora/classification-bundle": "2.4.1.*"

    },
    "require-dev": {
        "doctrine/orm": "*",
        "rmzamora/doctrine-orm-admin-bundle": "2.4.1.*",
        "rmzamora/notification-bundle": "2.4.1.*"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },    
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Sonata\\NewsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-2.4.1": "2.4.1.x-dev"
        }
    }
}
