{
    "name": "rmzamora/news-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataNewsBundle, rmzamora forked",
    "keywords": ["blog", "sonata", "news", "post", "rmzamora forked"],
    "homepage": "https://sonata-project.org/bundles/news",
    "license": "MIT",
    "authors": [
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataNewsBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/symfony": "~2.3|~3.0",
        "rmzamora/core-bundle": "2.5.0.*",
        "rmzamora/admin-bundle": "2.5.0.*",
        "rmzamora/user-bundle":  "2.5.0.*",
        "rmzamora/media-bundle": "2.5.0.*",
        "rmzamora/formatter-bundle": "2.5.0.*",
        "rmzamora/classification-bundle": "2.5.0.*",
        "rmzamora/datagrid-bundle": "2.5.0.*",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/intl-bundle": "~2.1"

    },
    "require-dev": {
        "doctrine/orm": "*",
        "rmzamora/doctrine-orm-admin-bundle": "2.5.0.*",
        "rmzamora/notification-bundle": "2.5.0.*",
        "rmzamora/seo-bundle": "2.5.0.*",
        "fabpot/php-cs-fixer": "~0.1|~1.0",
		"friendsofsymfony/rest-bundle": "~1.1",
        "jms/serializer-bundle": "~0.11|~1.0",
        "nelmio/api-doc-bundle": "~2.4"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Sonata\\NewsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-2.5.0": "2.5.0.x-dev"
        }
    }
